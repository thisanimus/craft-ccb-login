{
	"name": "thisanimus/craft-ccb-login",
	"description": "Log in via the CCB API, and log user info in session.",
	"type": "craft-plugin",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"craft-ccb-login"
	],
	"support": {
		"docs": "https://github.com/thisanimus/craft-ccb-login/blob/master/README.md",
		"issues": "https://github.com/thisanimus/craft-ccb-login/issues"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Andrew Hale",
			"homepage": "https://thisanimus.com"
		}
	],
	"require": {
		"craftcms/cms": "^3.0.0|^4.0.0-alpha",
		"thisanimus/ccb-api-connect": "^2.0.0"
	},
	"autoload": {
		"psr-4": {
			"thisanimus\\craftccblogin\\": "src/"
		}
	},
	"extra": {
		"name": "Craft CCB Login",
		"handle": "craft-ccb-login",
		"hasCpSettings": true,
		"hasCpSection": false,
		"changelogUrl": "https://raw.githubusercontent.com/thisanimus/craft-ccb-login/master/CHANGELOG.md",
		"components": {
			"craftccbloginService": "thisanimus\\craftccblogin\\services\\CraftccbloginService"
		},
		"class": "thisanimus\\craftccblogin\\Craftccblogin"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require-dev": {
		"craftcms/rector": "dev-main"
	},
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true
		}
	}
}
