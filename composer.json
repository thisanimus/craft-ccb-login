{
	"name": "thisanimus/craft-ccb-login",
	"description": "Log in via the CCB API, and log user info in session.",
	"type": "craft-plugin",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"ccb-login"
	],
	"support": {
		"docs": "https://github.com/thisanimus/ccb-login/blob/master/README.md",
		"issues": "https://github.com/thisanimus/ccb-login/issues"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Andrew Hale",
			"homepage": "https://thisanimus.com"
		}
	],
	"require": {
		"craftcms/cms": "^4.0.0-alpha",
		"thisanimus/ccb-api-connect": "^2.0.0"
	},
	"autoload": {
		"psr-4": {
			"thisanimus\\CCBLogin\\": "src/"
		}
	},
	"extra": {
		"name": "CCB Login",
		"handle": "ccb-login",
		"hasCpSettings": true,
		"hasCpSection": false,
		"changelogUrl": "https://raw.githubusercontent.com/thisanimus/ccb-login/master/CHANGELOG.md"
	}
}
